@model int
@{
    ViewBag.Title = "楼宇主页";
    Layout = null;
}

<div class="row-fluid">
    <div class="box span12">
        <div class="box-header well" data-original-title>
            <h2>
                <i class="icon-th-list"></i>楼宇</h2>
        </div>

        <div class="box-content">                      
            <div id="svg"></div>
        </div>
    </div>
</div>


<div class="row-fluid">
    <div class="span6">
        @Html.Action("Details", new { controller = "Building", area = "Estate", id = Model })
    </div>
    <div class="span6">
        @Html.Action("FloorList", new { controller = "Building", area = "Estate", buildingId = Model })
    </div>
</div>