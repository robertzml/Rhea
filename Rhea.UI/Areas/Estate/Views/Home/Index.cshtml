@section scripts {
<script type="text/javascript">
    $(function () {
        $('a#summary').click(function() {
            $('ul.nav').children().removeClass('active');
            $(this).parent().addClass('active');
            ajaxLoadPage("Summary", "Home", "Estate", {});
            return false;
        });

        $('a#building-group-list').click(function() {
            $('ul.nav').children().removeClass('active');
            $(this).parent().addClass('active');
            ajaxLoadPage("List", "BuildingGroup", "Estate", {});
            return false;
        });

        $('a#department-list').click(function () {
            $('ul.nav').children().removeClass('active');
            $(this).parent().addClass('active');
            ajaxLoadPage2("List", "Department", {});
            return false;
        });
    });
</script>
}